<?xml version="1.0" encoding="utf-8"?>
<Bundle xmlns="urn:schemas-geshe-com:osgi:bundle" Name="Company.Device" SymbolicName="Company.Device" Version="1.0.0.0" InitialState="Active" StartLevel="4">
  <Runtime>
    <Assembly Path="bin\Company.Device.dll" Share="true" Multiversion="false" />
    <!-- 模块依赖的其他模块，需要从本插件创建Company.Device.UI插件中的参数设置对话框等 -->
    <Dependency BundleSymbolicName="Company.Device.UI" />
  </Runtime>
  <Extension Point="Genesis.Device.Devices">
    <DeviceCategory Id="Company" Name="XX公司设备与接口">
      <Description>XX公司的设备与接口</Description>
    </DeviceCategory>
    
    <Device Id="CompanySerialXX" Category="Company" Name="XX型串口" Class="Company.Device.SerialSessionXX" EditorClass="Company.Device.UI.SerialSessionXXDialog">
      <Image></Image>
      <LargeImage></LargeImage>
      <Description>XX型串口-具有通用串口功能和增强串口功能</Description>
    </Device>
  </Extension>
</Bundle>